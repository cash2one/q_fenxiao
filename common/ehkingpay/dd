    hmacSource = ''
    hmacSource += ["merchantId"]
    hmacSource += ["requestId"]
    hmacSource += ["serialNumber"]
    hmacSource += ["totalRefundCount"]
    hmacSource += ["totalRefundAmount"]
    hmacSource += ["orderCurrency"]
    hmacSource += ["orderAmount"]
    hmacSource += ["status"]
    hmacSource += ["completeDateTime"]
    hmacSource += ["remark"]